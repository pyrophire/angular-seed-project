{
  "name": "CHANGEME",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "fresh": "rimraf node_modules && rimraf package-lock.json && npm i",
    "audit": "npm audit --json | npm-audit-html --output reports/build-audit.html ",
    "license-checker": "npm i -g npm-license-crawler && npm-license-crawler  --onlyDirectDependencies  --json src/assets/license.json",
    "update-cli": "ng update @angular/cli --force -C",
    "update-core": "ng update @angular/core --force -C",
    "update-material": "ng update @angular/material --force -C",
    "update-rxjs": "ng update rxjs --force -C",
    "update-all": "npm run update-cli && npm run update-core && npm run update-material && npm run update-rxjs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.1.7",
    "@angular/cdk": "^19.1.5",
    "@angular/common": "^19.1.7",
    "@angular/compiler": "^19.1.7",
    "@angular/core": "^19.1.7",
    "@angular/forms": "^19.1.7",
    "@angular/material": "^19.1.5",
    "@angular/platform-browser": "^19.1.7",
    "@angular/platform-browser-dynamic": "^19.1.7",
    "@angular/router": "^19.1.7",
    "@ng-select/ng-select": "^12.0.4",
    "@ngneat/bind-query-params": "^6.0.0",
    "@ngneat/hot-toast": "^6.0.1",
    "angular-rpns-client-library": "file:./libs/angular-rpns-client-library-0.13.1-alpha.tgz",
    "date-fns": "^2.30.0",
    "ix-libs": "file:./libs/ix-libs-19.0.0.tgz",
    "jwt-decode": "^4.0.0",
    "ngx-pipes": "^3.2.2",
    "npm-audit-html": "^1.5.0",
    "rxjs": "~7.8.2",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.1.8",
    "@angular/cli": "~19.1.8",
    "@angular/compiler-cli": "^19.1.7",
    "@ngneat/falso": "^7.2.0",
    "@ngneat/overview": "^5.1.1",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ngx-json-viewer": "^3.2.1",
    "typescript": "~5.7.3"
  }
}