{
  "name": "CHANGEME",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "fresh": "rimraf node_modules && rimraf package-lock.json && npm i",
    "audit": "npm audit --json | npm-audit-html --output reports/build-audit.html ",
    "license-checker": "npm i -g npm-license-crawler && npm-license-crawler  --onlyDirectDependencies  --json src/assets/license.json",
    "update-cli": "ng update @angular/cli --force -C",
    "update-core": "ng update @angular/core --force -C",
    "update-material": "ng update @angular/material --force -C",
    "update-rxjs": "ng update rxjs --force -C",
    "update-all": "npm run update-cli && npm run update-core && npm run update-material && npm run update-rxjs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.0.2",
    "@angular/cdk": "^18.0.2",
    "@angular/common": "^18.0.2",
    "@angular/compiler": "^18.0.2",
    "@angular/core": "^18.0.2",
    "@angular/forms": "^18.0.2",
    "@angular/material": "^18.0.2",
    "@angular/platform-browser": "^18.0.2",
    "@angular/platform-browser-dynamic": "^18.0.2",
    "@angular/router": "^18.0.2",
    "@ng-select/ng-select": "^12.0.4",
    "@ngneat/bind-query-params": "^6.0.0",
    "@ngneat/hot-toast": "^6.0.1",
    "angular-rpns-client-library": "file:./libs/angular-rpns-client-library-0.13.1-alpha.tgz",
    "date-fns": "^2.30.0",
    "ix-libs": "file:./libs/ix-libs-18.0.0.tgz",
    "ngx-pipes": "^3.2.2",
    "npm-audit-html": "^1.5.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.3",
    "@angular/cli": "~18.0.3",
    "@angular/compiler-cli": "^18.0.2",
    "@ngneat/overview": "^5.1.1",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ngx-json-viewer": "^3.2.1",
    "typescript": "~5.4.5"
  }
}